{
  "name": "free-real-estate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate:json": "rimraf api/schema.json && typescript-json-schema --required --strictNullChecks --noExtraProps api/api.ts API --out api/schema.json",
    "generate:prisma": "prisma generate",
    "generate:db": "prisma db push --preview-feature",
    "generate": "yarn generate:prisma && yarn generate:db && yarn generate:json",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:ts": "tsc --noEmit",
    "lint:vue": "vti diagnostics",
    "lint": "yarn lint:js && yarn lint:ts && yarn lint:vue",
    "run-script": "ts-node -P scripts/tsconfig.json",
    "seed:user": "yarn run-script scripts/createUser.ts",
    "seed:homes": "yarn run-script scripts/seedHomes.ts",
    "lint:commit": "eslint --fix && yarn lint",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue,ts}": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "optionalDependencies": {
    "pdf-fill-form": "^5.1.0"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/composition-api": "^0.22.4",
    "@paljs/plugins": "^2.13.0",
    "@prisma/client": "^2.19.0",
    "bcryptjs": "^2.4.3",
    "browser-image-compression": "^1.0.14",
    "cookie-parser": "^1.4.5",
    "cookie-universal-nuxt": "^2.1.4",
    "core-js": "^3.8.2",
    "crosswalk": "https://git.io/JthwB",
    "fast-deep-equal": "^3.1.3",
    "filepond": "^4.27.0",
    "filepond-plugin-file-validate-type": "^1.2.6",
    "filepond-plugin-image-preview": "^4.6.6",
    "filepond-plugin-image-resize": "^2.0.9",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.17",
    "nuxt": "^2.15.3",
    "swagger-ui-express": "^4.1.6",
    "vue-filepond": "^6.0.0"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.4",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/router-extras": "^1.1.1",
    "@nuxtjs/vuetify": "^1.11.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/fs-extra": "^9.0.8",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.22",
    "@types/nodemailer": "^6.4.0",
    "@types/rimraf": "^3.0.0",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "enquirer": "^2.3.6",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "execa": "^5.0.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.13",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prisma": "^2.19.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "supertest-fetch": "^1.4.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript-json-schema": "^0.48.0",
    "vti": "^0.1.1",
    "vue-jest": "^3.0.4"
  }
}
