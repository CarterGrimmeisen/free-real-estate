{
  "name": "free-real-estate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate:json": "rimraf api/schema.json && typescript-json-schema --required --strictNullChecks api/api.d.ts API --out api/schema.json",
    "generate:prisma": "prisma generate",
    "generate:db": "prisma db push --preview-feature",
    "generate": "yarn generate:prisma && yarn generate:db && yarn generate:json",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:ts": "tsc --noEmit",
    "lint": "yarn lint:js && yarn lint:ts",
    "run-script": "ts-node -P scripts/tsconfig.json",
    "seed:user": "yarn run-script scripts/createUser.ts",
    "seed:homes": "yarn run-script scripts/seedHomes.ts",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue,ts}": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.1",
    "@nuxtjs/auth-next": "5.0.0-1611071776.3646657",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/composition-api": "^0.18.1",
    "@prisma/client": "^2.16.0",
    "balm-ui": "^8.16.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.8.2",
    "crosswalk": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.17",
    "nuxt": "^2.14.12",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.22",
    "@types/nodemailer": "^6.4.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "enquirer": "^2.3.6",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.4.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prisma": "^2.16.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript-json-schema": "^0.48.0",
    "vue-jest": "^3.0.4"
  }
}
